<div class="context-nav context-nav-brand" role="navigation" aria-label="Breadcrumb">
    <div class="container">
        <ol class="breadcrumb">
            <li>
				<%= link_to( 'UK Parliament', 'https://www.parliament.uk/' ) %>
            </li>
            <li>
				<%= link_to( 'House of Commons Library', 'https://commonslibrary.parliament.uk/' ) %>
            </li>
			<% if @crumb.empty? -%>
				<li>
	                <%= $SITE_TITLE %>
				</li>
			<% else -%>
				<li>
					<%= link_to( $SITE_TITLE, home_url ) %>
				</li>
				<%= render :partial => 'layouts/header/crumb_item', collection: @crumb %>
			<% end -%>
        </ol>
    </div>
</div>